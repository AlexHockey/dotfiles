export TERM=xterm-256color
source /usr/lib/git-core/git-sh-prompt
export PS1='\[\033[01;32m\]\u@\h\[\033[01;34m\] \w\[\033[01;35m\]$(__git_ps1)\[\033[01;34m\] \$\[\033[00m\] '

ssh-add ~/.ssh/mobile_engineering_keypair >/dev/null 2>&1
